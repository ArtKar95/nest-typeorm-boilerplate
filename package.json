{
  "name": "eatnact",
  "version": "1.0.0",
  "description": "Eatnact backend API",
  "author": " ",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npx ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/main",
    "start:docker": "npm run build && pm2 start ecosystem.config.js --no-daemon",
    "pm2:start": "npm run build && sudo pm2 start ecosystem.config.js",
    "pm2:stop": "sudo pm2 delete ecosystem.config.js",
    "test": "jest",
    "build": "nest build",
    "prebuild": "rimraf dist",
    "postinstall": "husky install",
    "pre-commit": "npx lint-staged",
    "prestart:prod": "rimraf dist && npm run build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- migration:create -n",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier": "prettier --check '*/**/*.{js,ts,tsx}'",
    "prettier:fix": "prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.1",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "8.0.2",
    "@types/node-jose": "^1.1.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.2",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "nestjs-redis": "^1.3.3",
    "node-jose": "^2.1.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.45",
    "url-join": "^5.0.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.5.1",
    "@types/jest-json-schema": "^6.1.0",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.6",
    "@types/node": "^17.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jsdoc": "^39.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.0",
    "jest-json-schema": "^6.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
